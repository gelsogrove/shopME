{
  "userManagement": {
    "disabled": false,
    "emails": {
      "mode": "smtp",
      "smtp": {
        "host": "localhost",
        "port": 587,
        "secure": false
      }
    }
  },
  "security": {
    "auditEvents": {
      "save": false
    }
  },
  "workflows": {
    "callerPolicyDefaultOption": "workflowsFromSameOwner"
  },
  "executions": {
    "saveDataOnError": "all",
    "saveDataOnSuccess": "all",
    "saveDataManualExecutions": true
  },
  "logging": {
    "level": "info",
    "outputs": ["console"]
  }
} 